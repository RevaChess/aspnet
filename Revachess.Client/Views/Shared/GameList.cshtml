<div class="mx-auto align-middle" style="width: 300px;">
    <h2>Users</h2>
    @foreach (var item in ViewBag.Users)
    {
        <div class="row" style="padding: 10px;">
            @item.UserName
            <br>
            <button type="button" class="btn btn-primary btn-sm">Play</button>
        </div>
    }
</div>
